(executable
 (name printmod)
 (modules printmod)
 (libraries unix threads)
 (link_deps ../target/debug/libdmz.a allocpair.o ../target/debug/examples/main.a)
 (link_flags -thread -ccopt -pthread target/debug/libdmz.a caml/allocpair.o target/debug/examples/main.a))

(rule
 (targets allocpair.o)
 (deps (:source allocpair.c))
 (action (run %{ocamlopt} -thread -ccopt -pthread %{source} -o %{targets})))
